{
    "rewrites": [
      {
        "source": "/api/v1/(.*)",
        "destination": "/api/server.js"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Custom-Header",
            "value": "my-custom-header"
          }
        ]
      }
    ]
  }
  